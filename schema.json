{
    "Namespaces": {
        "": "http://netj.org/2011/example",
        "xsd": "http://www.w3.org/2001/XMLSchema"
    },
    "Objects": {
        "user": {
            "Links": {
                "wrote": ["post"]
            },
            "Attributes": {
                "id": "xsd:string",
                "name": "xsd:string"
            },
            "Label": "name"
        },
        "post": {
            "Links": {
                "contains-text": ["text"]
            },
            "Attributes": {
                "created": "xsd:dateTime"
            }
        },
        "text": {
            "Links": {
                "contains-url": ["url"]
            },
            "Attributes": {
                "content": "xsd:string"
            }
        },
        "url": {
            "Links": {
            },
            "Attributes": {
                "url": "xsd:anyURI",
                "is-malicious": "xsd:boolean"
            }
        },
        "fan-page": {
            "Links": {
                "has-post": ["post"]
            },
            "Attributes": {
                "name": "xsd:string",
                "url": "xsd:anyURI"
            }
        }
    },
    "TypeLabels": {
        "attribute-type": "it's label"
    }
}
